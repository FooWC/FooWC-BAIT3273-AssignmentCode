# FooWC-BAIT3273-AssignmentCode

# Install flask on EC2 
# Either add "sudo" before all commands or use "sudo su" first

#!/bin/bash
yum update -y
yum install git -y
git clone https://github.com/FooWC/FooWC-BAIT3273-AssignmentCode.git
cd FooWC-BAIT3273-AssignmentCode
pip3 install flask
pip3 install pymysql
pip3 install boto3
pip3 install jinja2
pip3 install cgi
python3 app.py
